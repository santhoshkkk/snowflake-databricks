select * from roles
Show roles

GRANT IMPORTED PRIVILEGES ON DATABASE SNOWFLAKE_SAMPLE_DATA TO ROLE READ;
GRANT IMPORTED PRIVILEGES ON SCHMEAS SNOWFLAKE_SAMPLE_DATA.TPCH_SF1 TO ROLE READ;

GRANT SELECT ON ALL TABLES IN SCHEMA SNOWFLAKE_SAMPLE_DATA.TPCH_SF1  to ROLE READ;




SHOW USERS

GRANT ROLE READ TO USER SARAS;

REVOKE  role ACCOUNTADMIN  FROM USER SARAS

REVOKE  role OWNERSHIP  FROM USER SARAS


CREATE USER SARAS
  PASSWORD = 'Saras@123'
  LOGIN_NAME = 'SARAS'
  DEFAULT_ROLE = 'READ'
  DEFAULT_WAREHOUSE = 'COMPUTE_WH'
  MUST_CHANGE_PASSWORD = FALSE;

GRANT USAGE ON WAREHOUSE COMPUTE_WH TO READ

GRANT OPERATE ON

